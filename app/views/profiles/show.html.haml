=link_to @profile.name, @profile.link
= link_to 'Обновить', profile_path(@profile, retest: true)
%table.posts
  %tr
    %td
      %h3
        Призоловские
    %td
      %h3
        Обычные
  %tr
    %td
      - @promo_posts.each do |post|
        = render partial: 'posts/post', :locals => { post: post } # change to render :post ?
    %td
      - @usual_posts.each do |post|
        = render partial: 'posts/post', :locals => { post: post }
